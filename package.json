{
  "name": "@metahub/stylelint-config",
  "description": "A set of shareable stylelint configs",
  "version": "1.2.0",
  "author": "Pierre Vanduynslager",
  "bugs": {
    "url": "https://github.com/vanduynslagerp/stylelint-config/issues"
  },
  "dependencies": {
    "stylelint": "^8.0.0",
    "stylelint-order": "^0.6.0",
    "stylelint-scss": "^2.0.1"
  },
  "devDependencies": {
    "@metahub/eslint-config": "^1.6.0",
    "ava": "^0.22.0",
    "babel-eslint": "^7.2.3",
    "codecov": "^2.3.0",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-eslint-comments": "^1.0.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-sort-class-members": "^1.2.0",
    "eslint-plugin-unicorn": "^2.1.2",
    "nyc": "^11.1.0",
    "p-event": "^1.3.0",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1"
  },
  "homepage": "https://github.com/vanduynslagerp/stylelint-config#readme",
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "stylelint": ">=8.0.0",
    "stylelint-order": ">=0.6.0",
    "stylelint-scss": ">=2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanduynslagerp/stylelint-config.git"
  },
  "scripts": {
    "clean": "rimraf coverage && rimraf .nyc_output",
    "lint": "eslint *-config.js index.js package.json test",
    "pretest": "npm run lint && npm run clean",
    "test": "nyc --all ava -v && nyc report --reporter=lcov",
    "codecov": "codecov",
    "prepublishOnly": "npm run test"
  },
  "nyc": {
    "include": [
      "{*-config,index}.js"
    ]
  }
}
