{
  "name": "@metahub/stylelint-config",
  "description": "A set of shareable stylelint configs",
  "version": "0.0.0-development",
  "author": "Pierre Vanduynslager",
  "bugs": {
    "url": "https://github.com/vanduynslagerp/stylelint-config/issues"
  },
  "dependencies": {
    "stylelint": "^8.0.0",
    "stylelint-order": "^0.6.0",
    "stylelint-scss": "^2.0.1"
  },
  "devDependencies": {
    "@metahub/eslint-config": "^1.6.2",
    "ava": "^0.22.0",
    "babel-eslint": "^7.2.3",
    "codecov": "^2.3.0",
    "conventional-changelog-metahub": "^1.0.1",
    "eslint": "^4.5.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-eslint-comments": "^1.0.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-sort-class-members": "^1.2.0",
    "eslint-plugin-unicorn": "^2.1.2",
    "nyc": "^11.1.0",
    "p-event": "^1.3.0",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "semantic-release": "^8.0.0",
    "sr-commit-analyzer": "^2.0.2",
    "sr-release-notes-generator": "^1.1.2"
  },
  "homepage": "https://github.com/vanduynslagerp/stylelint-config#readme",
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "include": [
      "{*-config,index}.js"
    ]
  },
  "peerDependencies": {
    "stylelint": ">=8.0.0",
    "stylelint-order": ">=0.6.0",
    "stylelint-scss": ">=2.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "path": "sr-commit-analyzer",
      "preset": "metahub",
      "releaseRules": "conventional-changelog-metahub/release-rules"
    },
    "generateNotes": {
      "path": "sr-release-notes-generator",
      "preset": "metahub"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanduynslagerp/stylelint-config.git"
  },
  "scripts": {
    "clean": "rimraf coverage && rimraf .nyc_output",
    "codecov": "codecov",
    "lint": "eslint *-config.js index.js package.json test",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint && npm run clean",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "nyc --all ava -v && nyc report --reporter=lcov"
  }
}
